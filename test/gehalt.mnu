parameter formObj

class gehaltMENU(formObj, name) of menubar(formObj, name)

    this.Datei = new Menu(this)
    with (this.Datei)
	text = "&Datei"
    endwith

    this.Hilfe = new Menu(this)
    with (this.Hilfe)
	text = "&Hilfe"
    endwith
endclass
